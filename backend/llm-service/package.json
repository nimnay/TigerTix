{
  "name": "llm-service",
  "version": "1.0.0",
  "scripts": {
    "start": "node server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)+(spec|test).js"
    ]
  },
  "dependencies": {
    "cookie-parser": "^1.4.7",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "cors": "^2.8.5",
    "sqlite3": "^5.1.7",
    "@google/generative-ai": "^0.21.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  }
}
